{
  "name": "explorer",
  "version": "1.0.0",
  "description": "Zen Protocol block explorer",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "heroku-postbuild": "cd client && npm install && npm run build && rm -rf node_modules",
    "setup": "node build_scripts/setup.js",
    "dev": "nodemon --inspect=0.0.0.0:5858 -w ./server --legacy-watch server/index.js",
    "setup:db": "npm run setup:db:test && npm run setup:db:development",
    "setup:db:test": "cross-env NODE_ENV=test npx sequelize db:create",
    "setup:db:development": "npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all",
    "pretest": "cross-env NODE_ENV=test npx sequelize db:migrate",
    "test": "cross-env NODE_ENV=test tape server/**/*.test.js | tap-bail | tap-spec",
    "coverage": "cross-env NODE_ENV=test nyc tape server/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenprotocol/explorer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zenprotocol/explorer/issues"
  },
  "homepage": "https://github.com/zenprotocol/explorer#readme",
  "dependencies": {
    "asset-require-hook": "^1.2.0",
    "axios": "^0.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "helmet": "^3.12.1",
    "http-status": "^1.2.0",
    "ignore-styles": "^5.0.1",
    "morgan": "^1.9.0",
    "nconf": "^0.10.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "sequelize": "^4.37.10",
    "url-loader": "^1.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.3.2",
    "@types/tape": "^4.2.32",
    "blue-tape": "^1.0.0",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^4.1.0",
    "nodemon": "^1.17.5",
    "nyc": "^12.0.2",
    "sequelize-cli": "^4.0.0",
    "supertest": "^3.1.0",
    "tap-bail": "^1.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1"
  },
  "nyc": {
    "include": [
      "server/**"
    ]
  }
}
