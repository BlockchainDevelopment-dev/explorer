{
  "name": "explorer",
  "version": "1.0.0",
  "description": "Zen Protocol block explorer",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "heroku-postbuild": "npm run heroku:client:build",
    "heroku:db:migrate": "NODE_ENV=production npx sequelize db:migrate",
    "heroku:client:build": "npm run client:build",
    "zen:registry": "npm config set @zen:registry https://www.myget.org/F/zenprotocol/npm/",
    "setup": "node build_scripts/setup.js",
    "dev": "nodemon --inspect=0.0.0.0:5858 -w ./server --legacy-watch server/index.js",
    "setup:db": "npm run setup:db:test && npm run setup:db:development",
    "setup:db:test": "cross-env NODE_ENV=test npx sequelize db:create",
    "setup:db:development": "npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all",
    "pretest": "cross-env NODE_ENV=test npx sequelize db:migrate",
    "test": "npm-run-all -p test:server test:worker",
    "test:server": "cross-env NODE_ENV=test tape server/**/**/*.test.js | tap-bail | tap-spec",
    "test:worker": "cross-env NODE_ENV=test tape worker/**/**/*.test.js | tap-bail | tap-spec",
    "coverage": "cross-env NODE_ENV=test nyc tape server/**/*.test.js",
    "client:build:css": "node-sass-chokidar src/ -o src/",
    "client:watch:css": "npm run client:build:css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "client:start:js": "react-scripts start",
    "client:start": "npm-run-all -p client:watch:css client:start:js",
    "client:build:js": "react-scripts build",
    "client:build": "npm-run-all client:build:css client:build:js",
    "client:test": "react-scripts test --env=jsdom",
    "client:lib:test": "babel-tape-runner src/lib/**/**/*.test.js | tap-bail | tap-spec",
    "client:eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenprotocol/explorer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zenprotocol/explorer/issues"
  },
  "homepage": "http://mywebsite.com/public",
  "dependencies": {
    "asset-require-hook": "^1.2.0",
    "axios": "^0.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-register": "^6.26.0",
    "balloon-css": "^0.5.0",
    "bech32": "^1.1.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.1",
    "bull": "^3.4.2",
    "classnames": "^2.2.6",
    "cors": "^2.8.4",
    "deepmerge": "^2.1.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "helmet": "^3.12.1",
    "http-status": "^1.2.0",
    "ignore-styles": "^5.0.1",
    "lodash.debounce": "^4.0.8",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "morgan": "^1.9.0",
    "nconf": "^0.10.0",
    "node-sass-chokidar": "^1.3.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-infinite-scroller": "^1.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-table": "^6.8.6",
    "sequelize": "^4.37.10",
    "url-loader": "^1.0.1",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.3.2",
    "@types/tape": "^4.2.32",
    "babel-tape-runner": "^2.0.1",
    "blue-tape": "^1.0.0",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^4.1.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "nyc": "^12.0.2",
    "sequelize-cli": "^4.0.0",
    "supertest": "^3.1.0",
    "tap-bail": "^1.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1"
  },
  "nyc": {
    "include": [
      "server/**"
    ]
  }
}
